<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{title}}</title>

    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.0/normalize.min.css.map'>
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css'>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="/css/global.css">

</head>

<body>
    <script src='https://cdnjs.cloudflare.com/ajax/libs/mousetrap/1.6.2/mousetrap.min.js'></script>
    <script>
        function DOMID(name) {
            return document.getElementById(name)
        }

        const toUrlEncoded = obj => Object.keys(obj).map(k => encodeURIComponent(k) + '=' + encodeURIComponent(obj[k])).join('&');

        function POSTPackage(payload, route) {
            let req = new XMLHttpRequest()
            req.open('POST', route)
            req.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");

            req.onreadystatechange = () => {
                if (this.readyState == 4 && this.status == 200) {
                    return true
                } else return false
            }

            req.send(toUrlEncoded(payload))
        }
    </script>
    <div class='container window'>
        {{> header}}
        {{{body}}}
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
</body>

<footer>
</footer>

</html>